{
  "extends": "eslint:recommended",
  "env": {
    "browser": true,
    "node": true,
    "es6": true
  },
  "plugins": [
    "react"
  ],
  "ecmaFeatures": {
    "jsx": true
  },
  "globals": {
    "chrome": true
  },
  "rules": {
    "indent": [2, 2, {"SwitchCase": 1}],
    "semi": [2, "always"],
    "quotes": [2, "single"],
    "eqeqeq": 2,
    "no-undef": 1,
    "no-console": 0
  }
}
