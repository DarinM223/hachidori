{
  "name": "animelist-checker",
  "version": "1.0.0",
  "description": "A minimalistic Hummingbird React.js anime list tracker ",
  "main": "background.js",
  "dependencies": {
    "async": "^0.9.0",
    "body-parser": "^1.13.1",
    "bootstrap": "^3.3.2",
    "bower": "^1.4.1",
    "express": "^4.13.0",
    "jquery": "^2.1.3",
    "react": "^0.12.2",
    "react-bootstrap": "^0.13.3",
    "superagent": "^1.1.0",
    "underscore": "^1.7.0"
  },
  "devDependencies": {
    "babel": "^5.6.4",
    "babelify": "^6.1.2",
    "browserify": "^8.1.3",
    "envify": "^3.2.0",
    "reactify": "^1.0.0",
    "watchify": "^2.3.0"
  },
  "browserify": {
    "transform": [
      "reactify",
      "envify"
    ]
